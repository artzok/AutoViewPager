<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AutoViewPager">
        <attr name="selectedDrawable" format="reference"/>
        <attr name="unselectedDrawable" format="reference"/>
        <attr name="intervalTime" format="integer"/>
        <attr name="indicatorWidth" format="dimension"/>
        <attr name="indicatorHeight" format="dimension"/>
        <attr name="indicatorPadding" format="dimension"/>
        <attr name="showPageTitle" format="boolean"/>
        <attr name="pageTitleFontSize" format="dimension"/>
        <attr name="pageTitleFontColor" format="color"/>
    </declare-styleable>
</resources>